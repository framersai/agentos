{
  "compilerOptions": {
    "composite": true,
    "rootDir": "src",
    "baseUrl": "./src",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ES2020",
    "esModuleInterop": true,
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@framers/agentos/*": ["./*"],
      "@prisma/client": ["./stubs/prismaClient.ts"],
      "dompurify": ["./stubs/dompurify.d.ts"]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}
