# agentos/.env.server.example

# AgentOS Server Configuration
AGENTOS_SERVER_PORT=3000
AGENTOS_SERVER_HOST=0.0.0.0
NODE_ENV=development

# Security
AGENTOS_JWT_SECRET=your-super-secret-jwt-key-change-this
AGENTOS_API_KEY_HEADER=x-agentos-api-key
AGENTOS_MASTER_API_KEY=your-master-api-key
AGENTOS_CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# Rate Limiting
AGENTOS_RATE_LIMIT_WINDOW_MS=60000
AGENTOS_RATE_LIMIT_MAX_REQUESTS=100
AGENTOS_RATE_LIMIT_SKIP_SUCCESS=false

# WebSocket Configuration
AGENTOS_WS_MAX_CONNECTIONS=1000
AGENTOS_WS_HEARTBEAT_INTERVAL=25000
AGENTOS_WS_CONNECTION_TIMEOUT=60000
AGENTOS_WS_MAX_MESSAGE_SIZE=1048576

# Logging
AGENTOS_LOG_LEVEL=info
AGENTOS_LOG_FORMAT=json
AGENTOS_ENABLE_REQUEST_LOGGING=true

# Monitoring
AGENTOS_METRICS_ENABLED=true
AGENTOS_METRICS_ENDPOINT=/metrics

# Webhooks
AGENTOS_WEBHOOKS_ENABLED=false
AGENTOS_WEBHOOK_ENDPOINTS=[]

# AgentOS Core Configuration (inherit from main .env)
# These would be loaded from the main AgentOS configuration